{
	"name": "Easy Task",
	"version": "1.0",
	"manifest_version": 2,
	
	"description": "Chrome extension to rank user's tasks by time to finish!",
	"permissions": ["storage", "declarativeContent", "activeTab", "identity"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},

	"page_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "images/get_started16.png",
	        "32": "images/get_started32.png",
	        "48": "images/get_started48.png",
	        "128": "images/get_started128.png"
		}
	},

	"icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },

    "options_page": "options.html",
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js https://cdn.jsdelivr.net/jquery.validation/1.16.0/jquery.validate.min.js https://cdn.jsdelivr.net/jquery.validation/1.16.0/additional-methods.min.js https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.24.0/moment.js; object-src 'self'",
    "key": "",
    "oauth2": {
    	"client_id": "960596868250-2ach0qv92q0gk00qlatsv80h8pjflmcu.apps.googleusercontent.com",
    	"scopes": [
    		"https://www.googleapis.com/auth/userinfo.profile"
    	]
    }
}